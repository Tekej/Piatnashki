<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="BackGround"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            stylesheets="/sample/css/style.css"
            fx:controller="sample.MainMenuController"
            prefHeight="400.0"
            prefWidth="550.0">
    <Text fx:id="GameTitle"
          layoutX="150.0"
          layoutY="75.0"
          text="PuzzleGame"/>
    <Button
            layoutX="242.0"
            layoutY="150.0"
            mnemonicParsing="false"
            onAction="#windowPassageSizeSelector"
            text="New Game"
    />
    <Button
            layoutX="240.0"
            layoutY="250.0"
            mnemonicParsing="false"
            onAction="#windowPassageHighScores"
            text="High Scores"
    />
    <Button fx:id="exit"
            layoutX="265.0"
            layoutY="350.0"
            mnemonicParsing="false"
            onAction="#exit"
            text="Exit"
    />
</AnchorPane>